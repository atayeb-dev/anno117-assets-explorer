{
    "loggers": {
        "default": {
            "animate": true,
            "debug": {
                "print_indent_chars": false
            }
        },
        "config": {
            "verbose": false
        },
        "cli": {
            "verbose": false,
            "animate": true,
            "data_print": {
                "styles": {
                    "str": "cc;di"
                }
            }
        },
        "traceback": {
            "animate": false,
            "data_print": {
                "styles": {
                    "objk": "cr",
                    "str": "cm"
                }
            },
            "verbose": false
        }
    },
    "unit_test_module": {
        "loggers": {
            "default": {
                "data_print": {
                    "decorate_items": false
                }
            }
        },
        "--modes": "data-print",
        "--test-data": "tests/medium-data.json tests/small-data.json"
    }
}